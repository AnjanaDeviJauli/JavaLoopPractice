package com.perscholas.java_basics;

import java.util.Scanner;

public class GreatestCommonDivisor {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int num1 = s.nextInt();
		int num2 = s.nextInt();
		int largestNumber;
		largestNumber = findTheLargestNumber(num1, num2);

		// finding the gcd
		for (int i = largestNumber; i > 0; i--) {
			if (num1 % i == 0 && num2 % i == 0) {
				System.out.printf("Gcd is %d", i);
				break;
			}

		}

	}

	public static int findTheLargestNumber(int a, int b) {
		if (a > b)
			return a;
		else
			return b;
	}

}
